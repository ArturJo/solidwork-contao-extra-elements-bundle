{
  "name": "solidwork/contao-extra-elements-bundle",
  "description": "Contao 5.3 bundle providing custom content elements and (later) frontend modules.",
  "type": "contao-bundle",
  "license": "MIT",
  "keywords": [
    "contao",
    "contao-bundle",
    "content-element",
    "twig",
    "custom-elements"
  ],
  "homepage": "https://github.com/ArturJo/solidwork-contao-extra-elements-bundle",
  "support": {
    "issues": "https://github.com/ArturJo/solidwork-contao-extra-elements-bundle/issues",
    "source": "https://github.com/ArturJo/solidwork-contao-extra-elements-bundle"
  },
  "authors": [
    {
      "name": "SolidWork",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "contao/core-bundle": "^5.3"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "SolidWork\\ContaoExtraElementsBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "SolidWork\\ContaoExtraElementsBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-main": "0.6.x-dev"
    }
  }
}